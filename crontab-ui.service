[Unit]
Description=WebUI fot crontab
After=network-online.target

[Service]
Restart=always
RestartSec=10
ExecStart=HOST=0.0.0.0 PORT=8000 /usr/bin/node ~/crontab-ui/node_modules/crontab-ui/bin/crontab-ui
StandardOutput=syslog               # Output to syslog
StandardError=syslog                # Output to syslog
SyslogIdentifier=CNCJS
User=pi

[Install]
WantedBy=multi-user.target